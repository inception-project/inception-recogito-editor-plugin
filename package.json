{
  "name": "inception-recogito-editor-plugin",
  "version": "0.0.1",
  "description": "RecogitoJS annotation editor plugin",
  "scripts": {
    "build:recogito": "esbuild src/index.ts --target=es6 --bundle --sourcemap --global-name=RecogitoEditor --outfile=dist/RecogitoEditor.js",
    "build:recogito-minified": "esbuild src/index.ts --minify --target=es6 --bundle --sourcemap --global-name=RecogitoEditor --outfile=dist/RecogitoEditor.min.js"
  },
  "external": {},
  "devDependencies": {
    "@recogito/recogito-js": "^1.6.0",
    "esbuild": "^0.13.12",
    "typescript": "^4.4.2"
  }
}
